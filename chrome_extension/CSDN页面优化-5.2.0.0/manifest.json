{
"update_url": "https://clients2.google.com/service/update2/crx",
  
  "name": "CSDN页面优化",
  "manifest_version":2,
  "version": "5.2",  
  "description": "自动显示博客全文，去掉登录提示",
  "icons":{
    "128": "images/icon128.png"
  },
  "browser_action": {  
    "default_icon": "./images/icon128.png",
    "default_title": "interest"
  },
  "content_scripts": [
    {
      "matches": ["https://blog.csdn.net/*"],
      "js": ["main.js"]
    }
  ]
}
